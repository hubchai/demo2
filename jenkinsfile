pipeline{
    agent any
tools {
  maven 'm1'
}
stages {
  stage('build') {
    steps {
      sh 'mvn clean install'
    }
  }

  stage('test') {
    steps {
      sh 'mvn test'
    }

    post {
      always {
       archiveArtifacts artifacts: 'target/**.jar', followSymlinks: false
      }
    }
  }

}


}
